{
    "name": "geofile",
    "version": "1.0.0",
    "description": "Library to handle Spatial Data file (shapefile, geojson, csv, ...)",
    "main": "index.js",
    "scripts": {
      "compile": "tsc -p .",
      "copy": "cp src/*.js src/*.css src/*.d.ts package.json ./dist ",
      "install_s": "cp dist/*.* ../server/node_modules/geofile ",
      "install_c": "cp dist/*.* ../client/node_modules/geofile ",
      "build": "run-s compile copy install_s install_c"
    },
    "author": "Mohamed Ben Zekri",
    "license": "ISC",
    "devDependencies": {
      "@types/node": "^10.11.6",
      "@types/ol": "^4.6.2",
      "npm-run-all": "^4.1.3",
      "typescript": "^3.1.2"
    },
    "dependencies": {
      "jsonparse": "^1.3.1",
      "ol": "^5.3.0"
    }
  }
  